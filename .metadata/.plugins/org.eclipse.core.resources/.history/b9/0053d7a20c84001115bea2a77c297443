
package com.igorcrevar.punchthecircle.android;

import android.os.Bundle;

import com.badlogic.gdx.backends.android.AndroidApplication;
import com.badlogic.gdx.backends.android.AndroidApplicationConfiguration;
import com.igorcrevar.punchthecircle.game.PunchTheCircle;

public class RollOverSphereActivity extends AndroidApplication {
	/** Called when the activity is first created. */
	@Override
	public void onCreate (Bundle savedInstanceState) {
		super.onCreate(savedInstanceState);
		AndroidApplicationConfiguration config = new AndroidApplicationConfiguration();
		config.useCompass = false;
		config.useAccelerometer = false;
		config.useGL20 = false;
		PunchTheCircle game = new PunchTheCircle();
		initialize(game, config);
	}
}
