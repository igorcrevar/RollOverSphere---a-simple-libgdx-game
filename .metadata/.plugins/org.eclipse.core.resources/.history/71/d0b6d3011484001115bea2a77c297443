package com.igorcrevar.rolloversphere.objects.boxes;

public enum BoxType {
	NOT_UPGRADE,
	UPGRADE_EASY_CHANGE_DIRECTION,
	UPGRADE_SPEED;
	
	public String getText(BoxType type){
		switch (upgradeType){
		case Box.UPGRADE_EASY_CHANGE_DIRECTION:
			mNotifText = "UPGRADE FAST DIRECTION CHANGE!";
			break;
		case Box.UPGRADE_SPEED:
			mNotifText = "UPGRADE SPEED!";
			break;
		}
	}
}
